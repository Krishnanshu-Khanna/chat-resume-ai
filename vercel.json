{
  "functions": {
    "src/app/(main)/interview/action.ts": {
      "maxDuration": 60
    }
  },
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://cvhelper.in/:path*",
      "permanent": true
    },
    {
      "source": "/:path*.html",
      "destination": "/:path*",
      "permanent": true
    },
    {
      "source": "/:path*.php",
      "destination": "/:path*",
      "permanent": true
    }
  ]
}
